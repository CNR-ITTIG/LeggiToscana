						</ep>
						<ep id="art1-com1-pun13">
							<num />
							<alinea>
								ZONA 13
								<ndr num="#n3" valore="" xlink:type="simple">(c)</ndr>
							</alinea>
							<ep id="art1-com1-pun13-pun1">
								<num />
