									<h:tbody>
										<h:tr>
											<h:td>- Riscossioni</h:td>
											<h:td>1.848.360.426</h:td>
										</h:tr>
										<h:tr>
											<h:td>- Pagamenti</h:td>
											<h:td>1.813.817.680</h:td>
										</h:tr>
										<h:tr>
											<h:td>Fondo di casa alla chiusura dell'es. 1982</h:td>
											<h:td>34.542.746</h:td>
										</h:tr>
										<h:tr>
											<h:td>- Residui attivi</h:td>
