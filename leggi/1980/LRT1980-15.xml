								<h:tbody>
									<h:tr>
										<h:td>In diminuzione</h:td>
										<h:td />
										<h:td />
									</h:tr>
									<h:tr>
										<h:td>3o Obiettivo: Sviluppo delle attività economiche</h:td>
										<h:td />
										<h:td />
									</h:tr>
									<h:tr>
										<h:td>3.9 Altri interventi</h:td>
										<h:td />
										<h:td />
									</h:tr>
									<h:tr>
										<h:td>Tipo di spesa 1-2-B</h:td>
										<h:td />
										<h:td />
									</h:tr>
									<h:tr>
										<h:td />
										<h:td>1981</h:td>
										<h:td>L. 20.000.000</h:td>
									</h:tr>
									<h:tr>
										<h:td />
										<h:td>1982</h:td>
										<h:td>L. 20.000.000</h:td>
									</h:tr>
									<h:tr>
										<h:td>Cap. 63000</h:td>
										<h:td>- Fondo globale per il finanziamento di spese per l'adempimento di funzioni normali (spese correnti)</h:td>
										<h:td>L.70.000.000</h:td>
									</h:tr>
									<h:tr>
										<h:td>In aumento</h:td>
										<h:td />
										<h:td />
									</h:tr>
									<h:tr>
										<h:td>3o Obiettivo: sviluppo delle attività economiche</h:td>
										<h:td />
										<h:td />
									</h:tr>
									<h:tr>
										<h:td>3.9 Altri interventi</h:td>
										<h:td />
										<h:td />
									</h:tr>
									<h:tr>
										<h:td>Tipo di spesa 1-2-A</h:td>
										<h:td />
										<h:td />
									</h:tr>
									<h:tr>
										<h:td />
										<h:td>1981</h:td>
										<h:td>L. 20.000.000</h:td>
									</h:tr>
									<h:tr>
										<h:td />
										<h:td>1982</h:td>
										<h:td>L. 20.000.000</h:td>
									</h:tr>
								</h:tbody>
							</h:table>
						</corpo>
					</comma>
					<comma id="art2-com3">
						<num />
						<corpo>
							Gli oneri relativi agli esercizi finanziari 1983 e 1984, nell'importo annuo indicato nell'
							<rif xlink:href="#art1" xlink:type="simple">art. 1</rif>
							, faranno carico ai corrispondenti capitoli di bilancio.
						</corpo>
					</comma>
