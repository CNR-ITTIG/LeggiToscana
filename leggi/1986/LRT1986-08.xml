														<h:td>dotazione (n. posti) 1060</h:td>
													</h:tr>
													<h:tr>
														<h:td>QF VII</h:td>
														<h:td>qualifica</h:td>
														<h:td>dotazione (n. posti) 910</h:td>
													</h:tr>
													<h:tr>
														<h:td>QF VI</h:td>
														<h:td>qualifica</h:td>
														<h:td>dotazione (n. posti) 1395</h:td>
													</h:tr>
													<h:tr>
														<h:td>QF V</h:td>
														<h:td>qualifica</h:td>
														<h:td>dotazione (n. posti) 272</h:td>
													</h:tr>
													<h:tr>
														<h:td>QF IV</h:td>
														<h:td>qualifica</h:td>
														<h:td>dotazione (n. posti) 883</h:td>
													</h:tr>
													<h:tr>
														<h:td>QF III</h:td>
														<h:td>qualifica</h:td>
														<h:td>dotazione (n. posti) 338</h:td>
													</h:tr>
