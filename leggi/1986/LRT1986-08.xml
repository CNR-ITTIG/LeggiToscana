												<h:tbody>
													<h:tr>
														<h:td>QF 2a</h:td>
														<h:td>qualifica dirigenziale</h:td>
														<h:td>dotazione (n. posti) 121</h:td>
													</h:tr>
													<h:tr>
														<h:td>QF 1a</h:td>
														<h:td>qualifica dirigenziale</h:td>
														<h:td>dotazione (n. posti) 374</h:td>
													</h:tr>
													<h:tr>
														<h:td>QF VIII</h:td>
