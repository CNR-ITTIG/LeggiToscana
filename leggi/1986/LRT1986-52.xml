									<h:tbody>
										<h:tr>
											<h:td>- Fondo cassa al 1.1.1984</h:td>
											<h:td>1.095.204.936</h:td>
										</h:tr>
										<h:tr>
											<h:td>- Riscossioni</h:td>
											<h:td>43.820.770.232</h:td>
										</h:tr>
