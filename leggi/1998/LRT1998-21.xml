			<formulainiziale>
				<h:p>Il Consiglio Regionale ha approvato il 18-3-1998</h:p>
				<h:p>Il Commissario del Governo ha apposto il visto il 9-4-1998</h:p>
				<h:p>Il Presidente della Giunta Regionale</h:p>
				<h:p>promulga la seguente legge regionale:</h:p>
			</formulainiziale>
			<articolato>
				<articolo id="art1">
					<num>ARTICOLO 1</num>
					<comma id="art1-com1">
						<num />
						<alinea>
							<mod id="mod1">
								La
								<rif xlink:href="urn:nir:regione.toscana:legge:1998-01-14;1#art7-com1-letb" xlink:type="simple">lettera b) del comma 1 dell'art. 7 della LR 14.1.1998 n. 1</rif>
								viene così modificata:
							</mod>
						</alinea>
						<ep id="art1-com1-pun1">
							<num>-</num>
							<corpo>
								<mod id="mod2">
									le parole "
									<virgolette id="mod2-vir1" tipo="parola">comma 2 dell'art. 9</virgolette>
									" vengono sostituite con le parole: "
									<virgolette id="mod2-vir2" tipo="parola">comma 1 dell'art. 9</virgolette>
									".
								</mod>
							</corpo>
						</ep>
					</comma>
					<comma id="art1-com2">
						<num />
						<alinea>
							<mod id="mod3">
								Il
								<rif xlink:href="urn:nir:regione.toscana:legge:1998-01-14;1#art14-com1" xlink:type="simple">comma 1 dell'art. 14 della LR 14.1.1998 n. 1</rif>
								viene così modificato:
							</mod>
						</alinea>
						<ep id="art1-com2-pun1">
							<num>-</num>
							<corpo>
								<mod id="mod4">
									le parole "
									<virgolette id="mod4-vir1" tipo="parola">anno 1997</virgolette>
									" vengono sostituite con le parole: "
									<virgolette id="mod4-vir2" tipo="parola">anno 1998</virgolette>
									".
								</mod>
							</corpo>
						</ep>
					</comma>
				</articolo>
			</articolato>
