										,
									</mod>
									<mod id="mod12">
										<rif xlink:href="urn:nir:regione.toscana:legge:1991-05-16;20#art13-com4" xlink:type="simple">comma 4</rif>
										,
									</mod>
									<mod id="mod13">
										<rif xlink:href="urn:nir:regione.toscana:legge:1991-05-16;20#art13-com5" xlink:type="simple">comma 5</rif>
										;
									</mod>
